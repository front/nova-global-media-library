{
  "name": "frontkom/nova-global-media-library",
  "description": "Laravel Nova field for image/gallery upload and library browsing.",
  "keywords": [
    "laravel",
    "nova",
    "media",
    "library",
    "frontkom"
  ],
  "authors": [
    {
      "name": "Marttin Notta",
      "email": "marttinnotta@optimistdigital.com",
      "role": "Original Developer"
    },
    {
      "name": "Michał Środek",
      "email": "michal.srodek@frontkom.com",
      "role": "Developer"
    },
    {
      "name": "Frontkom",
      "email": "hello@frontkom.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "intervention/image": "^2.4",
    "laravel/nova": "^2.0.11 || ^3.0",
    "php-ffmpeg/php-ffmpeg": "^0.17.0"
  },
  "autoload": {
    "psr-4": {
      "Frontkom\\NovaMediaLibrary\\": [
        "src/"
      ]
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Frontkom\\NovaMediaLibrary\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
